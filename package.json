    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.5.7",
    "yup": "^0.32.11"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "yarn lint"
    ],
    "**/*.{js,jsx,json,css,scss,md}": [
      "yarn prettier --write"
    ]
  }
