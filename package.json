  },
  "keywords": [
    "novel",
    "ukrainian",
    "react"
  ],
  "author": "andrian-kars",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrian-kars/novela/issues"
  },
  "homepage": "https://github.com/andrian-kars/novela#readme",
  "devDependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "classnames": "^2.3.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.57.1",
    "vite": "^4.0.3"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
